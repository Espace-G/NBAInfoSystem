{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      editPlayer: {},\n      teamList: [],\n      createPlayer: {},\n      data: {}\n    };\n  },\n  methods: {\n    create() {\n      let form = new FormData();\n      let img = this.$refs.img;\n      if (img.files[0] != null) {\n        form.append(\"img\", img.files[0]);\n      }\n      form.append(\"username\", \"zhangsan\");\n      form.append(\"cnname\", this.createPlayer.cnname);\n      form.append(\"enname\", this.createPlayer.enname);\n      form.append(\"nation\", this.createPlayer.nation);\n      form.append(\"height\", this.createPlayer.height);\n      form.append(\"weight\", this.createPlayer.weight);\n      form.append(\"age\", this.createPlayer.age);\n      form.append(\"tid\", this.createPlayer.tid);\n      let options = {\n        url: 'api/player/insertPlayer',\n        data: form,\n        method: 'post',\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      };\n      axios(options).then(response => {\n        if (response.data.status == 1) {\n          //创建成功，跳转player\n          this.$router.push(\"/player\");\n        }\n      });\n    },\n    getPlayer() {\n      axios.get(\"/api/player/selectPlayerById\", {\n        params: {\n          id: this.$route.query.pid\n        }\n      }).then(response => {\n        //console.log(response.data.obj)\n        this.editPlayer = response.data.obj;\n      });\n    },\n    getTeamList() {\n      axios.get(\"/api/team/query\").then(response => {\n        this.teamList = response.data.obj;\n      });\n    },\n    update() {\n      var params = new URLSearchParams();\n      params.append('pid', this.$route.query.pid);\n      params.append('cnname', this.editPlayer.cnname);\n      params.append('enname', this.editPlayer.enname);\n      params.append('height', this.editPlayer.height);\n      params.append('weight', this.editPlayer.weight);\n      params.append('nation', this.editPlayer.nation);\n      params.append('age', this.editPlayer.age);\n      params.append(\"tid\", this.editPlayer.tid);\n      axios.post(\"/api/player/update\", params).then(this.$router.go(-1));\n    },\n    deletePlayer() {\n      axios.get(\"/api/player/remove\", {\n        params: {\n          id: this.$route.query.pid\n        }\n      }).then(this.$router.go(-1));\n    },\n    test() {\n      this.$router.go(-1);\n    }\n  },\n  mounted() {\n    if (this.$route.query.pid != 0) {\n      this.getPlayer();\n      this.getTeamList();\n    } else {\n      this.getTeamList();\n      this.createPlayer = {\n        tid: 0,\n        cnname: \"\",\n        enname: \"\",\n        nation: \"\",\n        height: \"\",\n        weight: \"\",\n        age: \"\"\n      };\n    }\n  }\n};","map":{"version":3,"mappings":";AA8EA,OAAOA,KAAI,MAAO,OAAM;AACtB,eAAc;EACZC,IAAI,GAAE;IACJ,OAAM;MACJC,UAAU,EAAC,CAAC,CAAC;MACbC,QAAQ,EAAC,EAAE;MACXC,YAAY,EAAC,CAAC,CAAC;MACfH,IAAI,EAAC,CAAC;IACR;EACF,CAAC;EACDI,OAAO,EAAE;IACPC,MAAM,GAAE;MACN,IAAIC,IAAG,GAAI,IAAIC,QAAQ,EAAC;MACxB,IAAIC,GAAE,GAAI,IAAI,CAACC,KAAK,CAACD,GAAE;MACvB,IAAGA,GAAG,CAACE,KAAK,CAAC,CAAC,KAAK,IAAI,EAAC;QACtBJ,IAAI,CAACK,MAAM,CAAC,KAAK,EAACH,GAAG,CAACE,KAAK,CAAC,CAAC,CAAC;MAChC;MACAJ,IAAI,CAACK,MAAM,CAAC,UAAU,EAAC,UAAU;MACjCL,IAAI,CAACK,MAAM,CAAC,QAAQ,EAAC,IAAI,CAACR,YAAY,CAACS,MAAM;MAC7CN,IAAI,CAACK,MAAM,CAAC,QAAQ,EAAC,IAAI,CAACR,YAAY,CAACU,MAAM;MAC7CP,IAAI,CAACK,MAAM,CAAC,QAAQ,EAAC,IAAI,CAACR,YAAY,CAACW,MAAM;MAC7CR,IAAI,CAACK,MAAM,CAAC,QAAQ,EAAC,IAAI,CAACR,YAAY,CAACY,MAAM;MAC7CT,IAAI,CAACK,MAAM,CAAC,QAAQ,EAAC,IAAI,CAACR,YAAY,CAACa,MAAM;MAC7CV,IAAI,CAACK,MAAM,CAAC,KAAK,EAAC,IAAI,CAACR,YAAY,CAACc,GAAG;MACvCX,IAAI,CAACK,MAAM,CAAC,KAAK,EAAC,IAAI,CAACR,YAAY,CAACe,GAAG;MACvC,IAAIC,OAAM,GAAI;QACZC,GAAG,EAAE,yBAAyB;QAC9BpB,IAAI,EAAEM,IAAI;QACVe,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UACL,cAAc,EAAE;QACpB;MACF;MACAvB,KAAK,CAACoB,OAAO,CAAC,CAACI,IAAI,CAACC,QAAO,IAAK;QAC9B,IAAGA,QAAQ,CAACxB,IAAI,CAACyB,
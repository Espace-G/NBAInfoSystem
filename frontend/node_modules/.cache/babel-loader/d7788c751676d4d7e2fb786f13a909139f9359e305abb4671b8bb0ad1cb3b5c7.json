{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      players: [],\n      favorPlayers: [],\n      currentPage: 1,\n      totalRecord: 0,\n      up: 0,\n      down: 0,\n      searchText: \"\",\n      favorPid: [],\n      Pid: []\n    };\n  },\n  methods: {\n    getPlayers() {\n      axios.get(\"/api/player/selectPlayers\").then(response => {\n        //console.log(response)\n        this.players = response.data.obj;\n        for (var i = 0; i < this.players.length; i++) {\n          this.Pid[i] = this.players[i].pid;\n        }\n        this.currentPage = 1;\n        this.initPage();\n      });\n    },\n    pageAdd() {\n      if (this.up == this.totalRecord) {\n        alert(\"It is Last Page\");\n      } else {\n        this.currentPage++;\n        this.initPage();\n      }\n    },\n    pageSub() {\n      if (this.down == 0) {\n        alert(\"It is First Page\");\n      } else {\n        this.currentPage--;\n        this.initPage();\n      }\n    },\n    search() {\n      axios.get(\"/api/player/selectPlayers\", {\n        params: {\n          search: this.searchText\n        }\n      }).then(response => {\n        console.log(response.data.obj);\n        this.players = response.data.obj;\n        this.currentPage = 1;\n        this.initPage();\n      });\n    },\n    order(params) {\n      axios.get(\"/api/player/selectPlayers\", {\n        params: {\n          orderby: params\n        }\n      }).then(response => {\n        console.log(response.data);\n        console.log(response.data.obj);\n        this.players = response.data.obj;\n        this.currentPage = 1;\n        this.initPage();\n      });\n    },\n    getFavor() {\n      var uid;\n      axios.get(\"/api/user/getLogining\").then(response => {\n        //console.log(response.data.obj)\n        if (response.data.obj == null) {\n          //未登录\n          this.favorPlayers = [];\n        } else {\n          //已登录\n          uid = response.data.obj.uid;\n          axios.get(\"/api/favor/player\", {\n            params: {\n              id: uid\n            }\n          }).then(res => {\n            this.favorPlayers = res.data.obj;\n            for (var i = 0; i < this.favorPlayers.length; i++) {\n              this.favorPid[i] = this.favorPlayers[i].pid;\n            }\n          });\n        }\n      });\n    },\n    myfavor() {\n      var uid;\n      axios.get(\"/api/user/getLogining\").then(response => {\n        //console.log(response.data.obj)\n        if (response.data.obj == null) {\n          //未登录\n          if (confirm(\"Please Login First.\")) {\n            this.$router.push(\"/login\");\n          }\n        } else {\n          //已登录，将players替换为favorPlayers\n          this.players = this.favorPlayers;\n          this.initPage();\n        }\n      });\n    },\n    //每次players或currentPage有变化时调用，更新列表信息\n    initPage() {\n      this.totalRecord = this.players.length;\n      this.up = this.currentPage * 2 > this.totalRecord ? this.totalRecord : this.currentPage * 2;\n      this.down = (this.currentPage - 1) * 2 <= 0 ? 0 : (this.currentPage - 1) * 2;\n    },\n    follow(params) {\n      //alert(params)\n      var uid;\n      axios.get(\"/api/user/getLogining\").then(response => {\n        if (response.data.obj == null) {\n          //未登录\n          if (confirm(\"Please Login First.\")) {\n            this.$router.push(\"login\");\n          }\n        } else {\n          uid = response.data.obj.uid;\n          axios.get(\"/api/favor/create\", {\n            params: {\n              type: \"player\",\n              uid: uid,\n              id: params\n            }\n          }).then(res => {\n            console.log(res.data);\n            this.getFavor();\n          });\n        }\n      });\n    },\n    unfollow(params) {\n      var uid;\n      axios.get(\"/api/user/getLogining\").then(response => {\n        if (response.data.obj == null) {\n          //未登录\n          if (confirm(\"Please Login First.\")) {\n            this.$router.push(\"login\");\n          }\n        } else {\n          uid = response.data.obj.uid;\n          axios.get(\"/api/favor/undo\", {\n            params: {\n              type: \"player\",\n              uid: uid,\n              id: params\n            }\n          }).then(res => {\n            console.log(res.data);\n            this.getFavor();\n          });\n        }\n      });\n    }\n  },\n  watch: {\n    $route() {\n      this.getPlayers();\n    }\n  },\n  mounted() {\n    this.getPlayers();\n    this.getFavor();\n  }\n};","map":{"version":3,"mappings":";AAwGA,OAAOA,KAAI,MAAO,OAAM;AACtB,eAAc;EACZC,IAAI,GAAE;IACJ,OAAO;MACLC,OAAO,EAAC,EAAE;MACVC,YAAY,EAAC,EAAE;MACfC,WAAW,EAAC,CAAC;MACbC,WAAW,EAAC,CAAC;MACbC,EAAE,EAAC,CAAC;MACJC,IAAI,EAAC,CAAC;MACNC,UAAU,EAAC,EAAE;MACbC,QAAQ,EAAC,EAAE;MACXC,GAAG,EAAC;IACN;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,UAAU,GAAE;MACVZ,KAAK,CAACa,GAAG,CAAC,2BAA2B,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;QACtD;QACA,IAAI,CAACb,OAAM,GAAIa,QAAQ,CAACd,IAAI,CAACe,GAAE;QAC/B,KAAI,IAAIC,IAAI,CAAC,EAACA,IAAI,IAAI,CAACf,OAAO,CAACgB,MAAM,EAACD,CAAC,EAAE,EAAC;UACxC,IAAI,CAACP,GAAG,CAACO,CAAC,IAAI,IAAI,CAACf,OAAO,CAACe,CAAC,CAAC,CAACE,GAAE;QAClC;QACA,IAAI,CAACf,WAAU,GAAI;QACnB,IAAI,CAACgB,QAAQ,EAAC;MAChB,CAAC;IACH,CAAC;IACDC,OAAO,GAAE;MACP,IAAG,IAAI,CAACf,EAAC,IAAK,IAAI,CAACD,WAAW,EAAC;QAC7BiB,KAAK,CAAC,iBAAiB;MACzB,CAAC,MAAI;QACH,IAAI,CAAClB,WAAW,EAAC;QACjB,IAAI,CAACgB,QAAQ,EAAC;MAChB;IACF,CAAC;IACDG,OAAO,GAAE;MACP,IAAG,IAAI,CAAChB,IAAG,IAAK,CAAC,EAAC;QAChBe,KAAK,CAAC,kBAAkB;MAC1B,CAAC,MAAI;QACH,IAAI,CAAClB,WAAW,EAAC;QACjB,IAAI,CAACgB,QAAQ,EAAC;MAChB;IACF,CAAC;IACDI,MAAM,GAAE;MACNxB,KAAK,CAACa,GAAG,CAAC,2BAA2B,EAAC;QACpCY,MAAM,EAAC;UACLD,MAAM,EAAC,IAAI,CAAChB;QACd;MACF,CAAC,CAAC,CAACM,IAAI,CAACC,QAAO,IAAK;QAClBW,OAAO,CAACC,GAAG,CAACZ,QAAQ,CAACd,IAAI,CAACe,GAAG;QAC7B,IAAI,CAACd,OAAM,GAAIa,QAAQ,CAACd,IAAI,CAACe,GAAE;QAC/B,IAAI,CAACZ,WAAU,GAAI;QACnB,IAAI,CAACgB,QAAQ,EAAC;MAChB,CAAC;IACH,CAAC;IACDQ,KAAK,
{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      editPlayer: {},\n      teamList: [],\n      createPlayer: {\n        enname: \"\",\n        cnname: \"\",\n        height: \"\",\n        weight: \"\",\n        nation: \"\",\n        age: \"\"\n      },\n      data: {}\n    };\n  },\n  methods: {\n    imgChange() {\n      let form = new FormData();\n      let img = this.$refs.img;\n      form.append(\"img\", img.files[0]);\n      this.data = form;\n    },\n    create() {\n      this.data.append(\"cnname\", this.createPlayer.cnname);\n      this.data.append(\"enname\", this.createPlayer.enname);\n      this.data.append(\"nation\", this.createPlayer.nation);\n      this.data.append(\"height\", this.createPlayer.height);\n      this.data.append(\"weight\", this.createPlayer.weight);\n      this.data.append(\"age\", this.createPlayer.age);\n      let options = {\n        url: 'api/player/testSub',\n        data: this.img,\n        method: 'post',\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      };\n      axios(options).then(response => {\n        console.log(response.data);\n      });\n    },\n    getPlayer() {\n      axios.get(\"/api/player/selectPlayerById\", {\n        params: {\n          id: this.$route.query.pid\n        }\n      }).then(response => {\n        //console.log(response.data.obj)\n        this.editPlayer = response.data.obj;\n      });\n    },\n    getTeamList() {\n      axios.get(\"/api/team/query\").then(response => {\n        this.teamList = response.data.obj;\n      });\n    },\n    update() {\n      var params = new URLSearchParams();\n      params.append('pid', this.$route.query.pid);\n      params.append('cnname', this.editPlayer.cnname);\n      params.append('enname', this.editPlayer.enname);\n      params.append('height', this.editPlayer.height);\n      params.append('weight', this.editPlayer.weight);\n      params.append('nation', this.editPlayer.nation);\n      params.append('age', this.editPlayer.age);\n      params.append(\"tid\", this.editPlayer.tid);\n      axios.post(\"/api/player/update\", params).then(this.$router.go(-1));\n    },\n    deletePlayer() {\n      axios.get(\"/api/player/remove\", {\n        params: {\n          id: this.$route.query.pid\n        }\n      }).then(this.$router.go(-1));\n    },\n    test() {\n      this.$router.go(-1);\n    }\n  },\n  mounted() {\n    if (this.$route.query.pid != 0) {\n      this.getPlayer();\n      this.getTeamList();\n    } else {\n      this.getTeamList();\n    }\n  }\n};","map":{"version":3,"mappings":"AA2EA,OAAOA,KAAI,MAAO,OAAM;AACtB,eAAc;EACZC,IAAI,GAAE;IACJ,OAAM;MACJC,UAAU,EAAC,CAAC,CAAC;MACbC,QAAQ,EAAC,EAAE;MACXC,YAAY,EAAC;QACXC,MAAM,EAAC,EAAE;QACTC,MAAM,EAAC,EAAE;QACTC,MAAM,EAAC,EAAE;QACTC,MAAM,EAAC,EAAE;QACTC,MAAM,EAAC,EAAE;QACTC,GAAG,EAAC;MACN,CAAC;MACDT,IAAI,EAAC,CAAC;IACR;EACF,CAAC;EACDU,OAAO,EAAE;IACPC,SAAS,GAAE;MACT,IAAIC,IAAG,GAAI,IAAIC,QAAQ,EAAC;MACxB,IAAIC,GAAE,GAAI,IAAI,CAACC,KAAK,CAACD,GAAE;MACvBF,IAAI,CAACI,MAAM,CAAC,KAAK,EAACF,GAAG,CAACG,KAAK,CAAC,CAAC,CAAC;MAC9B,IAAI,CAACjB,IAAG,GAAIY,IAAG;IACjB,CAAC;IACDM,MAAM,GAAE;MACN,IAAI,CAAClB,IAAI,CAACgB,MAAM,CAAC,QAAQ,EAAC,IAAI,CAACb,YAAY,CAACE,MAAM;MAClD,IAAI,CAACL,IAAI,CAACgB,MAAM,CAAC,QAAQ,EAAC,IAAI,CAACb,YAAY,CAACC,MAAM;MAClD,IAAI,CAACJ,IAAI,CAACgB,MAAM,CAAC,QAAQ,EAAC,IAAI,CAACb,YAAY,CAACK,MAAM;MAClD,IAAI,CAACR,IAAI,CAACgB,MAAM,CAAC,QAAQ,EAAC,IAAI,CAACb,YAAY,CAACG,MAAM;MAClD,IAAI,CAACN,IAAI,CAACgB,MAAM,CAAC,QAAQ,EAAC,IAAI,CAACb,YAAY,CAACI,MAAM;MAClD,IAAI,CAACP,IAAI,CAACgB,MAAM,CAAC,KAAK,EAAC,IAAI,CAACb,YAAY,CAACM,GAAG;MAC5C,IAAIU,OAAM,GAAI;QACZC,GAAG,EAAE,oBAAoB;QACzBpB,IAAI,EAAE,IAAI,CAACc,GAAG;QACdO,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UACL,cAAc,EAAE;QACpB;MACF;MACAvB,KAAK,CAACoB,OAAO,CAAC,CAACI,IAAI,CAACC,QAAO,IAAK;QAC9BC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACxB,IAAI;MAC3B,CAAC;IACH,CAAC;IACD2B,SAAS,GAAE;MACT5B,KAAK,CAAC6B,GAAG,CAAC,8BAA8B,EAAC;QACvCC,MAAM,EAAC;UACLC,EAAE,EAAC,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC
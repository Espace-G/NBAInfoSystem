{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      tid: this.$route.query.tid,\n      editTeam: {},\n      createTeam: {},\n      errorMsg: \"\",\n      createTeam: {}\n    };\n  },\n  methods: {\n    create() {\n      let form = new FormData();\n      let img = this.$refs.img;\n      if (img.files[0] != null) {\n        form.append(\"logo_img\", img.files[0]);\n      }\n      form.append(\"username\", \"zhangsan\");\n      form.append(\"tname\", this.createTeam.tname);\n      form.append(\"tcity\", this.createTeam.tcity);\n      form.append(\"tzone\", this.createTeam.tzone);\n      form.append(\"arena\", this.createTeam.arena);\n      let options = {\n        url: 'api/team/insertPlayer',\n        data: form,\n        method: 'post',\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      };\n      axios(options).then(response => {\n        if (response.data.status == 0) {\n          //创建失败显示提示信息\n          this.errorMsg = \"Error Data!Please Check Again!\";\n        }\n      });\n    },\n    getTeam() {\n      axios.get(\"/api/team/selectById\", {\n        params: {\n          id: this.$route.query.tid\n        }\n      }).then(response => {\n        this.editTeam = response.data.obj;\n      });\n    },\n    update() {\n      var params = new URLSearchParams();\n      params.append(\"tname\", this.editTeam.tname);\n      params.append(\"tcity\", this.editTeam.tcity);\n      params.append(\"tzone\", this.editTeam.tzone);\n      params.append(\"arena\", this.editTeam.arena);\n      params.append(\"tid\", this.$route.query.tid);\n      axios.post(\"api/team/updateTeam\", params).then(response => {\n        console.log(response.data);\n      });\n    },\n    delete() {\n      axios.get(\"/api/team/removeTeam\", {\n        params: {\n          id: this.$route.query.tid\n        }\n      }).then(this.$router.go(-1));\n    }\n  },\n  mounted() {\n    if (this.$route.query.tid != 0) {\n      //edit\n      this.getTeam();\n    }\n  }\n};","map":{"version":3,"mappings":"AAkDA,OAAOA,KAAI,MAAO,OAAM;AACtB,eAAc;EACZC,IAAI,GAAE;IACJ,OAAO;MACLC,GAAG,EAAC,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,GAAG;MACzBG,QAAQ,EAAC,CAAC,CAAC;MACXC,UAAU,EAAC,CAAC,CAAC;MACbC,QAAQ,EAAC,EAAE;MACXD,UAAU,EAAC,CAAC;IACd;EACF,CAAC;EACDE,OAAO,EAAC;IACNC,MAAM,GAAE;MACN,IAAIC,IAAG,GAAI,IAAIC,QAAQ,EAAC;MACxB,IAAIC,GAAE,GAAI,IAAI,CAACC,KAAK,CAACD,GAAE;MACvB,IAAGA,GAAG,CAACE,KAAK,CAAC,CAAC,KAAK,IAAI,EAAC;QACtBJ,IAAI,CAACK,MAAM,CAAC,UAAU,EAACH,GAAG,CAACE,KAAK,CAAC,CAAC,CAAC;MACrC;MACAJ,IAAI,CAACK,MAAM,CAAC,UAAU,EAAC,UAAU;MACjCL,IAAI,CAACK,MAAM,CAAC,OAAO,EAAC,IAAI,CAACT,UAAU,CAACU,KAAK;MACzCN,IAAI,CAACK,MAAM,CAAC,OAAO,EAAC,IAAI,CAACT,UAAU,CAACW,KAAK;MACzCP,IAAI,CAACK,MAAM,CAAC,OAAO,EAAC,IAAI,CAACT,UAAU,CAACY,KAAK;MACzCR,IAAI,CAACK,MAAM,CAAC,OAAO,EAAC,IAAI,CAACT,UAAU,CAACa,KAAK;MACzC,IAAIC,OAAM,GAAI;QACZC,GAAG,EAAE,uBAAuB;QAC5BpB,IAAI,EAAES,IAAI;QACVY,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UACL,cAAc,EAAE;QACpB;MACF;MACAvB,KAAK,CAACoB,OAAO,CAAC,CAACI,IAAI,CAACC,QAAO,IAAK;QAC9B,IAAGA,QAAQ,CAACxB,IAAI,CAACyB,MAAK,IAAK,CAAC,EAAC;UAC3B;UACA,IAAI,CAACnB,QAAO,GAAI,gCAA+B;QACjD;MACF,CAAC;IACH,CAAC;IACDoB,OAAO,GAAE;MACP3B,KAAK,CAAC4B,GAAG,CAAC,sBAAsB,EAAC;QAC/BC,MAAM,EAAC;UACLC,EAAE,EAAC,IAAI,CAAC3B,MAAM,CAACC,KAAK,CAACF;QACvB;MACF,CAAC,CAAC,CAACsB,IAAI,CAACC,QAAO,IAAK;QAClB,IAAI,CAACpB,QAAO,GAAIoB,QAAQ,CAACxB,IAAI,CAAC8B,GAAE;MAClC,CAAC;IACH,CAAC;IACDC,MAAM,GAAE;MACN,IAAIH,MAAK,GAAI,IAAII,eAAe,EAAC;MACjCJ,MAAM,CAACd,MAAM,CAAC,OAAO,EAAC,IAAI,CAACV,QAAQ,CAACW,KAAK;MACzCa,MAAM,CAACd,MAAM,CAAC,OAAO,EAAC,IAAI,CAACV,QAAQ,CAACY,KAAK;MACzCY,MAAM,CAACd,MAAM,CAAC,OAAO,EAAC,IAAI,CAACV,QAAQ,CAACa,KAAK;MACzCW,MAAM,CAACd,MAAM,CAAC,OAAO,EAAC,IAAI,CAACV,QAAQ,CAACc,KAAK;MACzCU,MAAM,CAACd,MAAM,CAAC,KAAK,EAAC,IAAI,CAACZ,MAAM,CAACC,KAAK,CAACF,GAAG;MACzCF,KAAK,CAACkC,IAAI,CAAC,qBAAqB,EAACL,MAAM,CAAC,CAACL,IAAI,CAACC,QAAQ,IAAE;QACtDU,OAAO,CAACC,GAAG,CAACX,QAAQ,CAACxB,IAAI;MAC3B,CAAC;IACH,CAAC;IACDoC,MAAM,GAAE;MACNrC,KAAK,CAAC4B,GAAG,CAAC,sBAAsB,EAAC;QAC/BC,MAAM,EAAC;UACLC,EAAE,EAAC,IAAI,CAAC3B,MAAM,CAACC,KAAK,CAACF;QACvB;MACF,CAAC,CAAC,CAACsB,IAAI,CACL,IAAI,CAACc,OAAO,CAACC,EAAE,CAAC,CAAC,CAAC,EACpB;IACF;EACF,CAAC;EACDC,OAAO,GAAE;IACP,IAAG,IAAI,CAACrC,MAAM,CAACC,KAAK,CAACF,GAAE,IAAK,CAAC,EAAC;MAC5B;MACA,IAAI,CAACyB,OAAO,EAAC;IACf;EACF;AACF","names":["axios","data","tid","$route","query","editTeam","createTeam","errorMsg","methods","create","form","FormData","img","$refs","files","append","tname","tcity","tzone","arena","options","url","method","headers","then","response","status","getTeam","get","params","id","obj","update","URLSearchParams","post","console","log","delete","$router","go","mounted"],"sourceRoot":"","sources":["C:\\Users\\15312\\Desktop\\U\\大三上\\数据库\\课程设计\\NBAInfoSys\\frontend\\src\\components\\EditTeam.vue"],"sourcesContent":["<template>\r\n  <div class=\"d-flex flex-column align-content-end\">\r\n    <div class=\"app-auth-body mx-auto\">\t\r\n      <div class=\"app-auth-branding mb-4\">\r\n        <img class=\"logo-icon me-2\" style=\"height:70px\" src=\"img/NBALogo.png\" alt=\"logo\">\r\n      </div> \r\n      \r\n      <h2 class=\"auth-heading text-center mb-5\">Edit Team Information</h2>\r\n        <div class=\"auth-form-container text-start\">\r\n          <div v-show=\"(this.$route.query.tid != 0)\">\r\n            <label class=\"sr-only\">Team Name</label>\r\n            <input type=\"text\" v-model=\"editTeam.tname\" class=\"form-control \" placeholder=\"Team Name\">\r\n            <label class=\"sr-only\" >Team City</label>\r\n            <input type=\"text\" v-model=\"editTeam.tcity\" class=\"form-control \" placeholder=\"Team City\">\r\n            <label class=\"sr-only\" >Team Zone</label>\r\n            <input type=\"text\" v-model=\"editTeam.tzone\" class=\"form-control \" placeholder=\"Team Zone\">\r\n            <label class=\"sr-only\" >Arena</label>\r\n            <input type=\"text\" v-model=\"editTeam.arena\" class=\"form-control \" placeholder=\"Arena\">\r\n            <div class=\"text-center\">\r\n            <button class=\"btn app-btn-primary w-100 theme-btn mx-auto\" @click=\"update\">Update</button>\r\n            <hr>\r\n            <button class=\"btn app-btn-primary w-100 theme-btn mx-auto\" @click=\"deleteTeam\">Delete</button>\r\n          </div>\r\n          </div>\r\n          <div v-show=\"(this.$route.query.tid == 0)\">\r\n            <label class=\"sr-only\">Team Name</label>\r\n            <input type=\"text\" v-model=\"editTeam.tname\" class=\"form-control \" placeholder=\"Team Name\">\r\n            <label class=\"sr-only\" >Team City</label>\r\n            <input type=\"text\" v-model=\"editTeam.tcity\" class=\"form-control \" placeholder=\"Team City\">\r\n            <label class=\"sr-only\" >Team Zone</label>\r\n            <input type=\"text\" v-model=\"editTeam.tzone\" class=\"form-control \" placeholder=\"Team Zone\">\r\n            <label class=\"sr-only\" >Arena</label>\r\n            <input type=\"text\" v-model=\"editTeam.arena\" class=\"form-control \" placeholder=\"Arena\">\r\n            <div class=\"password mb-3\">\r\n              <label class=\"sr-only\" for=\"signin-password\">Player Img</label>\r\n              <input type=\"file\"  ref=\"img\" name=\"logo_img\" class=\"form-control \" placeholder=\"Img\" @change=\"imgChange\">\r\n            </div>\r\n            <div class=\"btn\">\r\n              <input type=\"submit\" class=\"form-control\" value=\"submit\" @click=\"create\">\r\n              <P style=\"color:red\"> {{ errorMsg }}</P>\r\n            </div>\r\n            \r\n          </div>\r\n        </div><!--//auth-form-container-->\t\r\n        \r\n    </div><!--//auth-body-->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n  export default{\r\n    data(){\r\n      return {\r\n        tid:this.$route.query.tid,\r\n        editTeam:{},\r\n        createTeam:{},\r\n        errorMsg:\"\",\r\n        createTeam:{},\r\n      }\r\n    },\r\n    methods:{\r\n      create(){\r\n        let form = new FormData()\r\n        let img = this.$refs.img\r\n        if(img.files[0] != null){\r\n          form.append(\"logo_img\",img.files[0])\r\n        }\r\n        form.append(\"username\",\"zhangsan\")\r\n        form.append(\"tname\",this.createTeam.tname)\r\n        form.append(\"tcity\",this.createTeam.tcity)\r\n        form.append(\"tzone\",this.createTeam.tzone)\r\n        form.append(\"arena\",this.createTeam.arena)\r\n        let options = {\r\n          url: 'api/team/insertPlayer',\r\n          data: form,\r\n          method: 'post',\r\n          headers: {\r\n              'Content-Type': 'multipart/form-data'\r\n          }\r\n        }\r\n        axios(options).then(response => {\r\n          if(response.data.status == 0){\r\n            //创建失败显示提示信息\r\n            this.errorMsg = \"Error Data!Please Check Again!\"\r\n          }\r\n        })\r\n      },\r\n      getTeam(){\r\n        axios.get(\"/api/team/selectById\",{\r\n          params:{\r\n            id:this.$route.query.tid\r\n          }\r\n        }).then(response => {\r\n          this.editTeam = response.data.obj\r\n        })\r\n      },\r\n      update(){\r\n        var params = new URLSearchParams()\r\n        params.append(\"tname\",this.editTeam.tname)\r\n        params.append(\"tcity\",this.editTeam.tcity)\r\n        params.append(\"tzone\",this.editTeam.tzone)\r\n        params.append(\"arena\",this.editTeam.arena)\r\n        params.append(\"tid\",this.$route.query.tid)\r\n        axios.post(\"api/team/updateTeam\",params).then(response=>{\r\n          console.log(response.data)\r\n        })\r\n      },\r\n      delete(){\r\n        axios.get(\"/api/team/removeTeam\",{\r\n          params:{\r\n            id:this.$route.query.tid\r\n          }\r\n        }).then(\r\n          this.$router.go(-1)\r\n        )\r\n      }\r\n    },\r\n    mounted(){\r\n      if(this.$route.query.tid != 0){\r\n        //edit\r\n        this.getTeam()\r\n      }\r\n    }\r\n  }\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
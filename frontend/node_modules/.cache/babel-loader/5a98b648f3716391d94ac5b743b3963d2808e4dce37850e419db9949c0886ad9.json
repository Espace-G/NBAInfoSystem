{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      tid: this.$route.query.tid,\n      createTeam: {},\n      createTeam: {},\n      errorMsg: \"\",\n      createTeam: {}\n    };\n  },\n  methods: {\n    create() {\n      let form = new FormData();\n      let img = this.$refs.img;\n      if (img.files[0] != null) {\n        form.append(\"img\", img.files[0]);\n      }\n      form.append(\"username\", \"zhangsan\");\n      form.append(\"tname\", this.createTeam.tname);\n      form.append(\"tcity\", this.createTeam.tcity);\n      form.append(\"tzone\", this.createTeam.tzone);\n      form.append(\"arena\", this.createTeam.arena);\n      let options = {\n        url: 'api/team/insertTeam',\n        data: form,\n        method: 'post',\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      };\n      axios(options).then(response => {\n        console.log(response.data);\n        if (response.data.status == 0) {\n          //创建失败显示提示信息\n          this.errorMsg = \"Error Data!Please Check Again!\";\n        } else {\n          this.$router.go(0);\n        }\n      });\n    },\n    getTeam() {\n      axios.get(\"/api/team/selectById\", {\n        params: {\n          id: this.$route.query.tid\n        }\n      }).then(response => {\n        this.createTeam = response.data.obj;\n      });\n    },\n    update() {\n      var params = new URLSearchParams();\n      params.append(\"tname\", this.createTeam.tname);\n      params.append(\"tcity\", this.createTeam.tcity);\n      params.append(\"tzone\", this.createTeam.tzone);\n      params.append(\"arena\", this.createTeam.arena);\n      params.append(\"tid\", this.$route.query.tid);\n      axios.post(\"api/team/updateTeam\", params).then(response => {\n        console.log(response.data);\n      });\n    },\n    deleteTeam() {\n      axios.get(\"/api/team/deleteTeam\", {\n        params: {\n          id: this.$route.query.tid\n        }\n      }).then(this.$router.go(-1));\n    }\n  },\n  mounted() {\n    if (this.$route.query.tid != 0) {\n      //edit\n      this.getTeam();\n    } else {\n      this.createTeam = {\n        tname: \"\",\n        tcity: \"\",\n        tzone: \"\",\n        arena: \"\"\n      };\n    }\n  }\n};","map":{"version":3,"mappings":"AAmDA,OAAOA,KAAI,MAAO,OAAM;AACtB,eAAc;EACZC,IAAI,GAAE;IACJ,OAAO;MACLC,GAAG,EAAC,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,GAAG;MACzBG,UAAU,EAAC,CAAC,CAAC;MACbA,UAAU,EAAC,CAAC,CAAC;MACbC,QAAQ,EAAC,EAAE;MACXD,UAAU,EAAC,CAAC;IACd;EACF,CAAC;EACDE,OAAO,EAAC;IACNC,MAAM,GAAE;MACN,IAAIC,IAAG,GAAI,IAAIC,QAAQ,EAAC;MACxB,IAAIC,GAAE,GAAI,IAAI,CAACC,KAAK,CAACD,GAAE;MACvB,IAAGA,GAAG,CAACE,KAAK,CAAC,CAAC,KAAK,IAAI,EAAC;QACtBJ,IAAI,CAACK,MAAM,CAAC,KAAK,EAACH,GAAG,CAACE,KAAK,CAAC,CAAC,CAAC;MAChC;MACAJ,IAAI,CAACK,MAAM,CAAC,UAAU,EAAC,UAAU;MACjCL,IAAI,CAACK,MAAM,CAAC,OAAO,EAAC,IAAI,CAACT,UAAU,CAACU,KAAK;MACzCN,IAAI,CAACK,MAAM,CAAC,OAAO,EAAC,IAAI,CAACT,UAAU,CAACW,KAAK;MACzCP,IAAI,CAACK,MAAM,CAAC,OAAO,EAAC,IAAI,CAACT,UAAU,CAACY,KAAK;MACzCR,IAAI,CAACK,MAAM,CAAC,OAAO,EAAC,IAAI,CAACT,UAAU,CAACa,KAAK;MACzC,IAAIC,OAAM,GAAI;QACZC,GAAG,EAAE,qBAAqB;QAC1BnB,IAAI,EAAEQ,IAAI;QACVY,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UACL,cAAc,EAAE;QACpB;MACF;MACAtB,KAAK,CAACmB,OAAO,CAAC,CAACI,IAAI,CAACC,QAAO,IAAK;QAC9BC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACvB,IAAI;QACzB,IAAGuB,QAAQ,CAACvB,IAAI,CAAC0B,MAAK,IAAK,CAAC,EAAC;UAC3B;UACA,IAAI,CAACrB,QAAO,GAAI,gCAA+B;QACjD,CAAC,MAAI;UACH,IAAI,CAACsB,OAAO,CAACC,EAAE,CAAC,CAAC;QACnB;MACF,CAAC;IACH,CAAC;IACDC,OAAO,GAAE;MACP9B,KAAK,CAAC+B,GAAG,CAAC,sBAAsB,EAAC;QAC/BC,MAAM,EAAC;UACLC,EAAE,EAAC,IAAI,CAAC9B,MAAM,CAACC,KAAK,CAACF;QACvB;MACF,CAAC,CAAC,CAACqB,IAAI,CAACC,QAAO,IAAK;QAClB,IAAI,CAACnB,UAAS,GAAImB,QAAQ,CAACvB,IAAI,CAACiC,GAAE;MACpC,CAAC;IACH,CAAC;IACDC,MAAM,GAAE;MACN,IAAIH,MAAK,GAAI,IAAII,eAAe,EAAC;MACjCJ,MAAM,CAAClB,MAAM,CAAC,OAAO,EAAC,IAAI,CAACT,UAAU,CAACU,KAAK;MAC3CiB,MAAM,CAAClB,MAAM,CAAC,OAAO,EAAC,IAAI,CAACT,UAAU,CAACW,KAAK;MAC3CgB,MAAM,CAAClB,MAAM,CAAC,OAAO,EAAC,IAAI,CAACT,UAAU,CAAC
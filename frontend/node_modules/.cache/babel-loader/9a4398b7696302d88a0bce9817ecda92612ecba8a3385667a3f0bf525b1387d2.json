s.append('age', this.editPlayer.age)\r\n        params.append(\"tid\",this.editPlayer.tid)\r\n        axios.post(\"/api/player/insertPlayer\",params).then(response =>{\r\n          console.log(response)\r\n        })\r\n      }\r\n    },\r\n    mounted(){\r\n      if(this.$route.query.pid != 0){\r\n        this.getPlayer()\r\n        this.getTeamList()\r\n      }else{\r\n        this.getTeamList()\r\n        this.editPlayer.cnname = \"\"\r\n        this.editPlayer.enname = \"\"\r\n        this.editPlayer.height = \"\"\r\n        this.editPlayer.weight = \"\"\r\n        this.editPlayer.nation = \"\"\r\n        this.editPlayer.tid = 1\r\n      }\r\n    }\r\n\r\n  }\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
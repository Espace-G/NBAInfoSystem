 {\n      this.totalRecord = this.players.length;\n      this.up = this.currentPage * 2 > this.totalRecord ? this.totalRecord : this.currentPage * 2;\n      this.down = (this.currentPage - 1) * 2 <= 0 ? 0 : (this.currentPage - 1) * 2;\n    }\n  },\n  watch: {\n    $route() {\n      this.getPlayers();\n    }\n  },\n  mounted() {\n    this.getPlayers();\n  }\n};","map":{"version":3,"mappings":";AAiGA,OAAOA,KAAI,MAAO,OAAM;AACtB,eAAc;EACZC,IAAI,GAAE;IACJ,OAAO;MACLC,OAAO,EAAC,EAAE;MACVC,WAAW,EAAC,CAAC;MACbC,WAAW,EAAC,CAAC;MACbC,EAAE,EAAC,CAAC;MACJC,IAAI,EAAC,CAAC;MACNC,UAAU,EAAC;IACb;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,UAAU,GAAE;MACVT,KAAK,CAACU,GAAG,CAAC,2BAA2B,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;QACtD;QACA,IAAI,CAACV,OAAM,GAAIU,QAAQ,CAACX,IAAI,CAACY,GAAE;QAC/B,IAAI,CAACV,WAAU,GAAI;QACnB,IAAI,CAACW,QAAQ,EAAC;MAChB,CAAC;IACH,CAAC;IACDC,OAAO,GAAE;MACP,IAAG,IAAI,CAACV,EAAC,IAAK,IAAI,CAACD,WAAW,EAAC;QAC7BY,KAAK,CAAC,iBAAiB;MACzB,CAAC,MAAI;QACH,IAAI,CAACb,WAAW,EAAC;QACjB,IAAI,CAACW,QAAQ,EAAC;MAChB;IACF,CAAC;IACDG,OAAO,GAAE;MACP,IAAG,IAAI,CAACX,IAAG,IAAK,CAAC,EAAC;QAChBU,KAAK,CAAC,kBAAkB;MAC1B,CAAC,MAAI;QACH,IAAI,CAACb,WAAW,EAAC;QACjB,IAAI,CAACW,QAAQ,EAAC;MAChB;IACF,CAAC;IACDI,MAAM,GAAE;MACNlB,KAAK,CAACU,GAAG,CAAC,2BAA2B,EAAC;QACpCS,MAAM,EAAC;UACLD,MAAM,EAAC,IAAI,CAACX;QACd;MACF,CAAC,CAAC,CAACI,IAAI,CAACC,QAAO,IAAK;QAClBQ,OAAO,CAACC,GAAG,CAACT,QAAQ,CAACX,IAAI,CAACY,GAAG;QAC7B,IAAI,CAACX,OAAM,GAAIU,QAAQ,CAACX,IAAI,CAACY,GAAE;QAC/B,IAAI,CAACV,WAAU,GAAI;QACnB,IAAI,CAACW,QAAQ,EAAC;MAChB,CAAC;IACH,CAAC;IACDQ,KAAK,CAACH,MAAM,EAAC;MACXnB,KAAK,CAACU,GAAG,CAAC,2BAA2B,EAAC;QACpCS,MAAM,EAAC;UACLI,OAAO,EAACJ;QACV;MACF,CAAC,CAAC,CAACR,IAAI,CAACC,QAAO,IAAK;QAClBQ,OAAO,CAACC,GAAG,CAACT,QAAQ,CAACX,IAAI;QACzBmB,OAAO,CAACC,GAAG,CAACT,QAAQ,CAACX,IAAI,CAACY,GAAG;QAC7B,IAAI,CAACX,OAAM,GAAIU,QAAQ,CAACX,IAAI,CAACY,GAAE;QAC/B,IAAI,CAACV,WAAU,GAAI;QACnB,IAAI,CAACW,QAAQ,EAAC;MAChB,CAAC;IACH,CAAC;IACDU,KAAK,GAAE;MACLxB,KAAK,CAACU,GAAG,CAAC,uBAAuB,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;QAClDQ,OAAO,CAACC,GAAG,CAACT,QAA